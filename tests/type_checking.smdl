#smdl

struct Foo {};

unit_test "Type checking" {
  auto foo = Foo();
  #assert(2 <: int);
  #assert(2.0f <: float);
  #assert(2.0d <: double);
  #assert("Hi!" <: string);
  #assert(foo <: Foo);
  #assert(#sizeof(int8_t) == 1);
  #assert(#sizeof(int16_t) == 2);
  #assert(#sizeof(int32_t) == 4);
  #assert(#sizeof(int64_t) == 8);
}
